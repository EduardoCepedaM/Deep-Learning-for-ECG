"""### RNN Y CNN 4"""

clf = Sequential()
#CNN
clf.add(Conv1D(filters=8, kernel_size=(3,), padding='same', activation='relu', input_shape = (X_train.shape[1],1)))
clf.add(Conv1D(filters=16, kernel_size=(3,), padding='same', activation='relu')) 
clf.add(Conv1D(filters=32, kernel_size=(5,), padding='same', activation='relu'))    

clf.add(MaxPool1D(pool_size=(3,), strides=2, padding='same'))
clf.add(Dropout(0.1))
#RNN
clf.add(LSTM(units = 32, return_sequences = True, input_shape = (X_train.shape[1],1)))
clf.add(LSTM(64,dropout=0.2, recurrent_dropout=0.2))
clf.add(Flatten())
#ANN
clf.add(Dense(units = 128, activation='relu'))
clf.add(Dense(units = 64, activation='relu'))

clf.add(Dense(units = 3, activation='softmax'))

clf.summary()

optimizer = tf.keras.optimizers.Adam(learning_rate=0.01)
clf.compile(optimizer= optimizer, loss = 'categorical_crossentropy', metrics=['accuracy'])

history = clf.fit(X_train, Y_train, validation_data=(X_test, y_test),
                    batch_size = 64,
                    epochs=300, verbose=0)

